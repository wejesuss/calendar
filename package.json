{
  "name": "calendar",
  "version": "0.0.0",
  "description": "Calendar",
  "main": "index.js",
  "scripts": {
    "start": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:parallel": "jest --passWithNoTests --silent --noStackTrace",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:clear": "jest --clearCache",
    "lint": "eslint src/** --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wejesuss/calendar.git"
  },
  "author": "Wemerson Jesus",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/wejesuss/calendar/issues"
  },
  "homepage": "https://github.com/wejesuss/calendar#readme",
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run test:staged"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.1",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.3.7",
    "prisma": "^3.12.0",
    "sucrase": "^3.21.0",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "express": "^4.18.1",
    "fast-glob": "^3.2.11",
    "validator": "^13.7.0"
  },
  "prisma": {
    "schema": "./src/infra/repositories/prisma/schema.prisma"
  }
}
